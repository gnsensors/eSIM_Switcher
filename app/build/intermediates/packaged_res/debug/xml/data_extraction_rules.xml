<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <exclude domain="sharedpref" path="device.xml"/>
        <exclude domain="sharedpref" path="com.google.android.gms.signin"/>
        <exclude domain="sharedpref" path="com.google.android.gms.appid"/>
        <exclude domain="database" path="com.google.android.datatransport.events"/>
    </cloud-backup>
    <device-transfer>
        <exclude domain="sharedpref" path="device.xml"/>
        <exclude domain="sharedpref" path="com.google.android.gms.signin"/>
        <exclude domain="sharedpref" path="com.google.android.gms.appid"/>
        <exclude domain="database" path="com.google.android.datatransport.events"/>
    </device-transfer>
</data-extraction-rules>